<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:smartForm="sap.ui.comp.smartform"
	xmlns:smartField="sap.ui.comp.smartfield">
	<Dialog id="InsertDialog" title="{i18n>insertTitle}">
		<content>
			<smartForm:SmartForm id="formDepSin" editable="true" entityType="ZPSTA_CFG_DEPARA_SIN">
				<smartForm:Group id="detailGroup">
					<smartForm:GroupElement>
						<smartField:SmartField id="codigo_evento_negocio" value="{CodEveNegocio}"/>
					</smartForm:GroupElement>
					<smartForm:GroupElement>
						<smartField:SmartField id="codigo_mov_sinistro" value="{CodMovSinistro}" />
					</smartForm:GroupElement>
					<smartForm:GroupElement>
						<smartField:SmartField id="juridico_flag" value="{JuridicoFlag}" />
					</smartForm:GroupElement>
					<smartForm:GroupElement>
						<smartField:SmartField id="tipo_movimento" value="{TipoMovimento}" />
					</smartForm:GroupElement>
					<smartForm:GroupElement>
						<smartField:SmartField id="tpmoid" value="{Tpmoid}" />
					</smartForm:GroupElement>
					<smartForm:GroupElement>
						<smartField:SmartField id="cmpid" value="{Cmpid}" />
					</smartForm:GroupElement>
					<smartForm:GroupElement>
						<smartField:SmartField id="tp_sin" value="{TpSin}" />
					</smartForm:GroupElement>
					<smartForm:GroupElement>
						<smartField:SmartField id="RO" value="{Ro}" />
					</smartForm:GroupElement>	
					<smartForm:GroupElement>
						<smartField:SmartField id="codigo_empresa" value="{CodEmpresa}" />
					</smartForm:GroupElement>
				</smartForm:Group>
			</smartForm:SmartForm>
		</content>
		<beginButton>
			<Button xmlns="sap.m" text="{i18n>btnAdd}" id="btnAdd" press=".onAdd"/>
		</beginButton>
		<endButton>
			<Button xmlns="sap.m" text="{i18n>btnCancel}" id="btnCancel" press=".onCancel('DepSin')"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>