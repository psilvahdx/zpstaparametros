<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:smartForm="sap.ui.comp.smartform" xmlns:form="sap.ui.layout.form"
	xmlns:smartField="sap.ui.comp.smartfield">
	<Dialog id="InsertDialog" title="{i18n>insertTitle}">
		<content>
			<smartForm:SmartForm id="formDepPrm" entityType="ZPSTA_CFG_DEPARA_PREM" editable="true">
				<smartForm:Group id="detailGroup">
					<smartForm:GroupElement>
						<smartField:SmartField value="{CodEveNegocio}" id="codigo_evento_negocio"/>
					</smartForm:GroupElement>
					<smartForm:GroupElement>
						<smartField:SmartField value="{CodigoEmpresa}" id="codigo_empresa"/>
					</smartForm:GroupElement>
					<smartForm:GroupElement>
						<smartField:SmartField value="{NumeroEndosso}" id="numero_endosso"/>
					</smartForm:GroupElement>
					<smartForm:GroupElement>
						<smartField:SmartField value="{Tpmoid}" id="tpmoid"/>
					</smartForm:GroupElement>
					<smartForm:GroupElement>
						<smartField:SmartField value="{Cmpid}" id="cmpid"/>
					</smartForm:GroupElement>
					<smartForm:GroupElement>
						<smartField:SmartField value="{Ro}" id="RO"/>
					</smartForm:GroupElement>
				</smartForm:Group>
			</smartForm:SmartForm>
		</content>
		<beginButton>
			<Button xmlns="sap.m" text="{i18n>btnAdd}" id="btnAdd" press=".onAdd"/>
		</beginButton>
		<endButton>
			<Button xmlns="sap.m" text="{i18n>btnCancel}" id="btnCancelDepPrm" press=".onCancel('DepPrm')"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>