<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:smartForm="sap.ui.comp.smartform" xmlns:form="sap.ui.layout.form"
	xmlns:smartField="sap.ui.comp.smartfield">
	<Dialog id="InsertDialogRamPass" title="{i18n>insertTitle}">
		<content>
			<smartForm:SmartForm id="formRamPass" editable="true" entityType="OZPSTA_CFG_GP_RAMO_PASSType">

				<smartForm:Group id="detailGroup">
					<smartForm:GroupElement>
						<smartField:SmartField value="{CodigoRamo}" id="codigo_ramo"/>
					</smartForm:GroupElement>
					<smartForm:GroupElement>
						<smartField:SmartField value="{GrupoRamoPassivo}" id="grupo_ramo_passivo"/>
					</smartForm:GroupElement>
					
				</smartForm:Group>
			</smartForm:SmartForm>
		</content>
		<beginButton>
			<Button xmlns="sap.m" text="{i18n>btnAdd}" id="btnAdd" press=".onAdd"/>
		</beginButton>
		<endButton>
			<Button xmlns="sap.m" text="{i18n>btnCancel}" id="btnCancelRamPass" press=".onCancel('RamPass')"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>