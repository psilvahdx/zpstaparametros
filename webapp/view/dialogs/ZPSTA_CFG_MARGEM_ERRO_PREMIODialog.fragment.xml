<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:smartForm="sap.ui.comp.smartform" xmlns:form="sap.ui.layout.form"
	xmlns:smartField="sap.ui.comp.smartfield">
	<Dialog id="InsertDialog" title="{i18n>insertTitle}">
		<content>
			<smartForm:SmartForm id="formMargErrPrem" editable="true" entityType="OZPSTA_CFG_MARGEM_ERRO_PREMIO">
				<smartForm:Group id="detailGroup">
					<smartForm:GroupElement label="{i18n>COD_EMPRESA}">
						<smartField:SmartField value="{CodigoEmpresa}" id="empresa"/>
					</smartForm:GroupElement>
					<smartForm:GroupElement label="{i18n>VAL_LIMITE_PREMIO}">
						<smartField:SmartField value="{ValorMargemLimite}" id="valor_margem_limite"/>
					</smartForm:GroupElement>
				</smartForm:Group>
			</smartForm:SmartForm>
		</content>
		<beginButton>
			<Button xmlns="sap.m" text="{i18n>btnAdd}" id="btnAdd" press=".onAdd"/>
		</beginButton>
		<endButton>
			<Button xmlns="sap.m" text="{i18n>btnCancel}" id="btnCancel" press=".onCancel"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>