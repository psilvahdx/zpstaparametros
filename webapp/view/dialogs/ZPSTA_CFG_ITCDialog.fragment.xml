<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:smartForm="sap.ui.comp.smartform"
    xmlns:form="sap.ui.layout.form" xmlns:smartField="sap.ui.comp.smartfield">
	<Dialog id="InsertDialog" title="{i18n>insertTitle}">
		<content>
			<smartForm:SmartForm id="formCfgItc" editable="true" entityType="ZPSTA_CFG_ITC">
				<smartForm:Group id="detailGroup">
					<smartForm:GroupElement>
						<smartField:SmartField value="{ItcCnpjRaiz}" id="itc_cnpj_raiz"/>
					</smartForm:GroupElement>
					<smartForm:GroupElement>
						<smartField:SmartField value="{ItcLegalEntity}" id="itc_legal_entity"/>
					</smartForm:GroupElement>
					<smartForm:GroupElement>
						<smartField:SmartField value="{ItcRazaoSocial}" id="itc_razao_social"/>
					</smartForm:GroupElement>
					<smartForm:GroupElement>
						<smartField:SmartField value="{ItcTipo}" id="itc_tipo"/>
					</smartForm:GroupElement>
				</smartForm:Group>
			</smartForm:SmartForm>
		</content>
		<beginButton>
			<Button xmlns="sap.m" text="{i18n>btnAdd}" id="btnAdd" press=".onAdd"/>
		</beginButton>
		<endButton>
			<Button xmlns="sap.m" text="{i18n>btnCancel}" id="btnCancelCfgItc" press=".onCancel('CfgItc')"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>