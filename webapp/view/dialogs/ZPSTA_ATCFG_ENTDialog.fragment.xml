<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:smartForm="sap.ui.comp.smartform" 
    xmlns:form="sap.ui.layout.form" xmlns:smartField="sap.ui.comp.smartfield">
	<Dialog id="InsertDialog" title="{i18n>insertTitle}">
		<content>
			<smartForm:SmartForm id="form" editable="true" entityType="OZPSTA_ATCFG_ENT">
				<smartForm:Group id="detailGroup">
					<smartForm:GroupElement>
						<smartField:SmartField value="{CodEmpresa}" id="cod_empresa"/>
					</smartForm:GroupElement>
					<smartForm:GroupElement>
						<smartField:SmartField value="{NomeEmpresa}" id="nome_empresa"/>
					</smartForm:GroupElement>
					<smartForm:GroupElement>
						<smartField:SmartField value="{CodLegalEntity}" id="cod_legal_entity"/>
					</smartForm:GroupElement>
					<smartForm:GroupElement>
						<smartField:SmartField value="{DtInclusaoReg}" id="dt_inclusao_reg"/>
					</smartForm:GroupElement>
					<smartForm:GroupElement>
						<smartField:SmartField value="{Status}" id="status"/>
					</smartForm:GroupElement>
					<smartForm:GroupElement>
						<smartField:SmartField value="{Client}" id="client"/>
					</smartForm:GroupElement>
				</smartForm:Group>
			</smartForm:SmartForm>
		</content>
		<beginButton>
			<Button xmlns="sap.m" text="{i18n>btnAdd}" id="btnAdd" press=".onAdd"/>
		</beginButton>
		<endButton>
			<Button xmlns="sap.m" text="{i18n>btnCancel}" id="btnCancel" press=".onCancel"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>