<mvc:View controllerName="portoseguro.zpstaparametros.controller.Fechamento"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:c="sap.ui.core"
	xmlns:u="sap.ui.unified"
	xmlns="sap.m"
	xmlns:t="sap.ui.table">
	<Page
		id="page"
		title="{i18n>OZPSTA_FECHAMENTODesc}"
		showHeader="true"
		enableScrolling="true"
		class="sapUiContentPadding"
		backgroundDesign="Solid"
		showNavButton="true" navButtonPress="onNavBack"
		>
		<content>
			<t:Table				
				rows="{/OZPSTA_FECH_CONTABSet}" 
				selectionMode="MultiToggle"
				visibleRowCount="13"
				paste="onPaste"				
				ariaLabelledBy="title"				
				id="idFechamentoTable">
				<t:extension>
					<OverflowToolbar id="_IDGenOverflowToolbar1" design="Auto">
						<Title id="title" text="---" titleStyle="H4"/>
						<ToolbarSpacer id="_IDGenToolbarSpacer1"/>
						<Button id="_IDGenButton1" press="onFilterDialog" type="Emphasized" icon="sap-icon://filter" tooltip="Filtro" visible="true"/>
						<!--	Importação de registros em massa
						<u:FileUploader id="fileUploader" name="myFileUpload" uploadUrl="upload/" visible="false" buttonText="Importar"
							tooltip="Seleciona o arquivo csv" change="onUpload" uploadComplete="handleUploadComplete" style="Default" enabled="true"/>
						<Button press="onImport" type="Transparent" id="importBtn" tooltip="Importar" icon="sap-icon://upload" visible="true"/>-->
						<Button id="_IDGenButton2" press="onAdd" type="Transparent" icon="sap-icon://add" tooltip="Adicionar" visible="true"/>
						<Button id="_IDGenButton3" press="onDelMessage" type="Transparent" icon="sap-icon://delete" tooltip="Excluir" visible="true"/>
						<Button id="_IDGenButton4" press="onEdit" type="Transparent" icon="sap-icon://edit" tooltip="Editar" visible="true"/>
						<Button id="_IDGenButton5" press="onExport" type="Transparent" icon="sap-icon://excel-attachment" tooltip="Exportar" visible="true"/>
						<!--<Button press="" type="Transparent" icon="sap-icon://sys-help" tooltip="Ajuda" visible="true"/>-->
					</OverflowToolbar>
				</t:extension>
				<t:columns>
					<t:Column id="_IDGenColumn1" width="7%">
						<Label id="_IDGenLabel1" text="Empresa"/>
						<t:template>
							<Text id="_IDGenText1" text="{Empresa}" wrapping="false" />
						</t:template>
					</t:Column>
					<t:Column id="_IDGenColumn2" width="10%">
						<Label id="_IDGenLabel2" text="Evento Negócio" />
						<t:template>
							<Text id="_IDGenText2" text="{CodEveNegocio}" wrapping="false" />
						</t:template>
					</t:Column>
					<t:Column id="_IDGenColumn3">
						<Label id="_IDGenLabel3" text="Ano"/>
						<t:template>
							<Text id="_IDGenText3" text="{Ano}" wrapping="false" />
						</t:template>
					</t:Column>
					<t:Column id="_IDGenColumn4">
						<Label id="_IDGenLabel4" text="Mês"/>
						<t:template>
							<Text id="_IDGenText4" text="{Mes}" wrapping="false" />
						</t:template>
					</t:Column>
					<t:Column id="_IDGenColumn5" width="13%">
						<Label id="_IDGenLabel5" text="Data de inclusão" />
						<t:template>
							<Text id="_IDGenText5" text="{ path: 'DataInclusao', formatter: '.formatter.date' }" wrapping="false" />
						</t:template>
					</t:Column>
					<t:Column id="_IDGenColumn6" width="7%">
						<Label id="_IDGenLabel6" text="Hora" />
						<t:template>
							<Text id="_IDGenText6" text="{ path: 'HoraInclusao', type: 'sap.ui.model.odata.type.Time',  formatOptions: { format: 'Hms' } }" wrapping="false" />
						</t:template>
					</t:Column>
					<t:Column id="_IDGenColumn7" hAlign="Center">
						<Label id="_IDGenLabel7" text="Jan"/>
						<t:template>
							<CheckBox id="_IDGenCheckBox1"  selected="{Mes01}" enabled="false"/>
						</t:template>
					</t:Column>
					<t:Column id="_IDGenColumn8" hAlign="Center">
						<Label id="_IDGenLabel8" text="Fev"/>
						<t:template>
							<CheckBox id="_IDGenCheckBox2"  selected="{Mes02}" enabled="false"/>
						</t:template>
					</t:Column>
					<t:Column id="_IDGenColumn9" hAlign="Center">
						<Label id="_IDGenLabel9" text="Mar"/>
						<t:template>
						<CheckBox id="_IDGenCheckBox3"  selected="{Mes03}" enabled="false"/>
						</t:template>
					</t:Column>
					<t:Column id="_IDGenColumn10" hAlign="Center">
						<Label id="_IDGenLabel10" text="Abr" />
						<t:template>
							<CheckBox id="_IDGenCheckBox4"  selected="{Mes04}" enabled="false"/>
						</t:template>
					</t:Column>
					<t:Column id="_IDGenColumn11" hAlign="Center">
						<Label id="_IDGenLabel11" text="Mai" />
						<t:template>
						<CheckBox id="_IDGenCheckBox5"  selected="{Mes05}" enabled="false"/>
						</t:template>
					</t:Column>
					<t:Column id="_IDGenColumn12" hAlign="Center">
						<Label id="_IDGenLabel12" text="Jun" />
						<t:template>
						<CheckBox id="_IDGenCheckBox6"  selected="{Mes06}" enabled="false"/>
						</t:template>
					</t:Column>
					<t:Column id="_IDGenColumn13" hAlign="Center">
						<Label id="_IDGenLabel13" text="Jul"  />
						<t:template>
						<CheckBox id="_IDGenCheckBox7"  selected="{Mes07}" enabled="false"/>
						</t:template>
					</t:Column>
					<t:Column id="_IDGenColumn14" hAlign="Center">
						<Label id="_IDGenLabel14" text="Ago" />
						<t:template>
						<CheckBox id="_IDGenCheckBox8"  selected="{Mes08}" enabled="false"/>
						</t:template>
					</t:Column>
					<t:Column id="_IDGenColumn15" hAlign="Center">
						<Label id="_IDGenLabel15" text="Set" />
						<t:template>
							<CheckBox id="_IDGenCheckBox9"  selected="{Mes09}" enabled="false"/>
						</t:template>
					</t:Column>
					<t:Column id="_IDGenColumn16" hAlign="Center">
						<Label id="_IDGenLabel16" text="Out" />
						<t:template>
						<CheckBox id="_IDGenCheckBox10"  selected="{Mes10}" enabled="false"/>
						</t:template>
					</t:Column>
					<t:Column id="_IDGenColumn17" hAlign="Center">
						<Label id="_IDGenLabel17" text="Nov" />
						<t:template>
							<CheckBox id="_IDGenCheckBox11"  selected="{Mes11}" enabled="false"/>
						</t:template>
					</t:Column>
					<t:Column id="_IDGenColumn18" hAlign="Center">
						<Label id="_IDGenLabel18" text="Dez" />
						<t:template>
							<CheckBox id="_IDGenCheckBox12"  selected="{Mes12}" enabled="false"/>
						</t:template>
					</t:Column>
					
					<!--	Colunas que não serão exibidas na tela		-->
					<!--<Column visible="false">
						<Label text="Usuário Criação" />
						<template>
							<Text text="{USUARIO_CRIACAO}" wrapping="false" />
						</template>
					</Column>
					<Column visible="false">
						<Label text="Data Criação" />
						<template>
							<Text text="{DATA_CRIACAO}" wrapping="false" />
						</template>
					</Column>
