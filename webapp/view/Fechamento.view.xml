						<template>
							<Text text="{DATA_MODIF}" wrapping="false" />
						</template>
					</Column>
					<Column visible="false">
						<Label text="Hora Modificação" />
						<template>
							<Text text="{HORA_MODIF}" wrapping="false" />
						</template>
					</Column>-->
				</t:columns>
			</t:Table>
		</content>
	</Page>
</mvc:View>