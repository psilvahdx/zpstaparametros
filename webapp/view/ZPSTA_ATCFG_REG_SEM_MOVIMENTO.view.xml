					</smartFilterBar:SmartFilterBar>
				</f:DynamicPageHeader>
			</f:header>
			<f:content>
				<smartTable:SmartTable entitySet="OZPSTA_ATCFG_REG_SEM_MOV" app:useSmartField="true" smartFilterId="smartFilterBarRegSMov"
					editTogglable="false" editable="false" tableType="Table" useExportToExcel="true" beforeExport="onBeforeExport" useVariantManagement="false"
