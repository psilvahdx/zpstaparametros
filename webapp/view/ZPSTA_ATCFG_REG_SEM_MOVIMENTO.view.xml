								<Button xmlns="sap.m" id="btnDel" icon="sap-icon://delete" press=".onDelete"/>
								<Button xmlns="sap.m" id="btnEdit" tooltip="Editar" icon="sap-icon://edit" press=".onEdit"/>
							</content>
						</OverflowToolbar>
					</smartTable:customToolbar>
				</smartTable:SmartTable>
			</f:content>
		</f:DynamicPage>
