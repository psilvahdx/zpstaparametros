<mvc:View controllerName="portoseguro.zpstaparametros.controller.zpsta_cfg_depara_premio" 
xmlns="sap.m"
xmlns:mvc="sap.ui.core.mvc" 
xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
xmlns:smartTable="sap.ui.comp.smarttable" 
xmlns:smartFilterBar="sap.ui.comp.smartfilterbar" 
xmlns:l="sap.ui.layout"
xmlns:f="sap.f">
    <Page id="depara_premio" title="{i18n>zpsta_cfg_depara_premioTitle}" showNavButton="true" navButtonPress=".onNavBack">
		<f:DynamicPage id="dynamicPageId">
			<f:header>
				<f:DynamicPageHeader pinnable="true">
					<smartFilterBar:SmartFilterBar id="smartFilterBarDepPrm" entitySet="OZPSTA_CFG_DEPARA_PREM" persistencyKey="SmartFilter_Explored">
						<smartFilterBar:controlConfiguration>
							<!--	<smartFilterBar:ControlConfiguration key="codigo_atena"></smartFilterBar:ControlConfiguration>
							<smartFilterBar:ControlConfiguration key="data"></smartFilterBar:ControlConfiguration>-->
						</smartFilterBar:controlConfiguration>
					</smartFilterBar:SmartFilterBar>
				</f:DynamicPageHeader>
			</f:header>
			<f:content>
				<smartTable:SmartTable entitySet="OZPSTA_CFG_DEPARA_PREM" app:useSmartField="true" smartFilterId="smartFilterBarDepPrm" editTogglable="false"
					editable="false" tableType="Table" useExportToExcel="true" beforeExport="onBeforeExport" useVariantManagement="false"
					useTablePersonalisation="true" header="Qtd. Registros" showRowCount="true" persistencyKey="SmartTableAnalytical_Explored"
					enableAutoBinding="true" class="sapUiResponsiveContentPadding" id="tblDadosDepPrm" showFullScreenButton="true" dataReceived="onDataReceived">
					<l:VerticalLayout class="sapUiContentPadding" id="oVerticalContent" width="100%"></l:VerticalLayout>
					<smartTable:layoutData>
						<FlexItemData id="FlexItemDataId" growFactor="1" baseSize="0%"/>
					</smartTable:layoutData>
					<smartTable:customToolbar>
						<OverflowToolbar width="100%" id="toolbar2">
							<content>
								<ToolbarSpacer id="toolBarSpacerID"/>
								<Button xmlns="sap.m" id="btnNew" icon="sap-icon://add" press=".onNew"/>
								<Button xmlns="sap.m" id="btnDel" icon="sap-icon://delete" press=".onDelete"/>
								<Button xmlns="sap.m" id="btnSave" icon="sap-icon://save" press=".onSave"/>
							</content>
						</OverflowToolbar>
					</smartTable:customToolbar>
				</smartTable:SmartTable>
			</f:content>
		</f:DynamicPage>
	</Page>
</mvc:View>