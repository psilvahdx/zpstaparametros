{
    "_version": "1.37.0",
    "sap.app": {
        "id": "portoseguro.zpstaparametros",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.7.6",
            "toolsId": "f00756d6-dc44-4816-b03f-89ce03326419"
        },
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/sap/ZPSGW_CONFIG_SRV/",
                "type": "OData",
                "settings": {
                    "annotations": [
                        "annotation"
                    ],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "FechamentoService": {
				"uri": "/sap/opu/odata/sap/ZPSTA_CDS_FECHAMENTO_CDS/",
				"type": "OData",
				"settings": {
                    "type": "ODataAnnotation",
					"localUri": "localService/metadata.xml"
				}
			},"annotation": {
				"type": "ODataAnnotation",
				"uri": "annotations/annotation.xml",
				"settings": {
					"localUri": "annotations/annotation.xml"
				}
			}
        },
        "crossNavigation": {
            "inbounds": {
                "portoseguro-zpstaparametros-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "ZSOFPSL",
                    "action": "zpsta_params",
                    "title": "{{flpTitle}}",
                    "subTitle": "{{flpSubtitle}}",
                    "icon": ""
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.96.6",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "portoseguro.zpstaparametros.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "defaultOperationMode": "Server",
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Request",
                    "defaultUpdateMethod": "PUT",
                    "useBatch": true
                }
            },
			"FechamentoServ": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request",
					"useBatch": false
				},
				"dataSource": "FechamentoService",
				"preload": true
			},
            "@i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			}
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "portoseguro.zpstaparametros.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteHome",
                    "pattern": ":?query:",
                    "target": [
                        "TargetHome"
                    ]
                },
                {
                    "name": "cfg_scripts",
                    "pattern": "cfg_scripts",
                    "target": "cfg_scripts"
                },
                {
                    "pattern": "emp_eve",
                    "name": "emp_eve",
                    "target": "emp_eve"
                }, 
                {
                    "pattern": "ramo_pass",
                    "name": "ramo_pass",
                    "target": "ramo_pass"
                }, 
                {
                    "pattern": "depara_premio",
                    "name": "depara_premio",
                    "target": "depara_premio"
                }, {
                    "pattern": "cfg_itc",
                    "name": "cfg_itc",
                    "target": "cfg_itc"
                },
                {
                    "pattern": "depara_sinistro",
                    "name": "depara_sinistro",
                    "target": "depara_sinistro"	
                },{
                    "pattern": "cfg_ent",
                    "name": "cfg_ent",
                    "target": "cfg_ent"	
                },
                {
                    "pattern": "cfg_origem",
                    "name": "cfg_origem",
                    "target": "cfg_origem"	
                },
                {
                    "pattern": "depara_empresa",
                    "name": "depara_empresa",
                    "target": "depara_empresa"	
                },
                {
                    "pattern": "reg_sem_movimento",
                    "name": "reg_sem_movimento",
                    "target": "reg_sem_movimento"	
                },
                {
                    "pattern": "margem_limite_premio",
                    "name":"margem_limite_premio",
                    "target": "margem_limite_premio"
                },
                {
                    "pattern": "desemp_libera_ctrl",
                    "name":"desemp_libera_ctrl",
                    "target": "desemp_libera_ctrl"
                },
                {
                    "pattern": "desemp_emails",
                    "name":"desemp_emails",
                    "target": "desemp_emails"
                },
                {
                    "pattern": "fechamento_desempenho",
                    "name":"fechamento_desempenho",
                    "target": "fechamento_desempenho"
                },
                {
                    "pattern": "fechamento_contabil",
                    "name":"fechamento_contabil",
                    "target": "fechamento_contabil"
                }
            ],
            "targets": {
                "TargetHome": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Home",
                    "viewName": "Home"
                },
                "cfg_scripts": {
					"viewId": "cfg_scripts",
					"viewName": "ZPSTA_CFG_SCRIPTS",
					"viewLevel": 2
				},
                "emp_eve": {
					"viewId": "emp_eve",
					"viewName": "ZPSTA_CFG_EMP_EVE",
					"viewLevel": 2
				},
				"ramo_pass": {
					"viewId": "ramo_pass",
					"viewName": "ZPSTA_CFG_GP_RAMO_PASS",
					"viewLevel": 2
				},
                "depara_premio": {
					"viewId": "depara_premio",
					"viewName": "ZPSTA_CFG_DEPARA_PREMIO",
					"viewLevel": 2
                },
                "depara_sinistro": {
					"viewId": "depara_sinistro",
					"viewName": "ZPSTA_CFG_DEPARA_SINISTRO",
					"viewLevel": 2
				},
				"cfg_itc": {
					"viewId": "cfg_itc",
					"viewName": "ZPSTA_CFG_ITC",
					"viewLevel": 2
				},
				"cfg_ent": {
					"viewId": "cfg_ent",
					"viewName": "ZPSTA_ATCFG_ENT",
					"viewLevel": 2
				},
				"cfg_origem": {
					"viewId": "cfg_origem",
					"viewName": "ZPSTA_ATCFG_ORIGEM",
					"viewLevel": 2
				},
                "depara_empresa": {
					"viewId": "depara_empresa",
					"viewName": "ZPSTA_CFG_DEPARA_EMPRESA",
					"viewLevel": 2
				},
                "reg_sem_movimento": {
					"viewId": "reg_sem_movimento",
					"viewName": "ZPSTA_ATCFG_REG_SEM_MOVIMENTO",
					"viewLevel": 2
				},
                "margem_limite_premio":{
					"viewId": "margem_limite_premio",
					"viewName": "ZPSTA_CFG_MARGEM_ERRO_PREMIO",
					"viewLevel": 2
				},
				"desemp_libera_ctrl":{
					"viewId": "desemp_libera_ctrl",
					"viewName": "ZPSTA_DESEMP_LIBERA_CTRL",
					"viewLevel": 2	
				},
				"desemp_emails":{
					"viewId": "desemp_emails",
					"viewName": "ZPSTA_DESEMP_EMAILS",
					"viewLevel": 2	
				},
				"fechamento_desempenho":{
					"viewId": "fechamento_desempenho",
					"viewName": "Fechamento_Desempenho",
					"viewLevel": 2	
				},				
				"fechamento_contabil":{
					"viewId": "fechamento_contabil",
					"viewName": "Fechamento",
					"viewLevel": 2
				}
            }
        },
        "rootView": {
            "viewName": "portoseguro.zpstaparametros.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        }
    }
}